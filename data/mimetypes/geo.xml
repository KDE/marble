    <comment xml:lang="eu">ESRI forma-fitxategiaren indizea</comment>
    <comment xml:lang="fi">ESRI Shapefile -hakemisto</comment>
    <comment xml:lang="fr">Index du fichier de formes « ESRI »</comment>
    <comment xml:lang="it">Indice Shapefile ESRI</comment>
    <comment xml:lang="ko">ESRI 모양 파일 인덱스</comment>
    <comment xml:lang="nl">Index vormbestand ESRI</comment>
    <comment xml:lang="nn">ESRI-shapefile-indeks</comment>
    <comment xml:lang="pa">ESRI ਸ਼ਕਲ-ਫਾਇਲ ਇੰਡੈਕਸ</comment>
    <comment xml:lang="pl">Indeks pliku kształtu ESRI</comment>
    <comment xml:lang="pt">Índice de formas da ESRI</comment>
    <comment xml:lang="pt_BR">Índice de shapefile ESRI</comment>
    <comment xml:lang="ru">Индекс файла фигуры ESRI</comment>
    <comment xml:lang="sk">Index súboru tvarov ESRI</comment>
    <comment xml:lang="sl">Kazalo ESRI Shapefile</comment>
    <comment xml:lang="sv">ESRI-formfilindex</comment>
    <comment xml:lang="uk">покажчик файла форм ESRI</comment>
    <comment xml:lang="zh_CN">ESRI 形状文件索引</comment>
    <generic-icon name="application-octet-stream"/>
    <glob pattern="*.shx"/>
    <magic priority="50">
      <match offset="0" type="big32" value="0x0000270a"/>
    </magic>
  </mime-type>
</mime-info>
