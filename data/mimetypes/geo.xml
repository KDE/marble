<?xml version="1.0" encoding="UTF-8"?>
<!--
Notes:
- the mime types in this file are valid with the version 0.40 of the
  shared-mime-info package.
-->
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-gpx+xml">
    <sub-class-of type="application/xml"/>
    <comment>GPX geographic data</comment>
    <comment xml:lang="ca">Dades geogràfiques GPX</comment>
    <comment xml:lang="ca@valencia">Dades geogràfiques GPX</comment>
    <comment xml:lang="cs">Zeměpisná data GPX</comment>
    <comment xml:lang="de">GPX-Geografiedaten</comment>
    <comment xml:lang="en_GB">GPX geographic data</comment>
    <comment xml:lang="es">Datos geográficos GPX</comment>
    <comment xml:lang="eu">GPX datu geografikoak</comment>
    <comment xml:lang="fi">GPX-paikkatieto</comment>
    <comment xml:lang="fr">Données géographiques « GPX »</comment>
    <comment xml:lang="it">Dati geografici GPX</comment>
    <comment xml:lang="ko">GPX 지리 데이터</comment>
    <comment xml:lang="lt">GPX geografiniai duomenys</comment>
    <comment xml:lang="nl">Geografische gegevens GPX</comment>
    <comment xml:lang="nn">GPX-geografidata</comment>
    <comment xml:lang="pa">GPX ਭੂਗੋਲਿਕ ਡਾਟਾ</comment>
    <comment xml:lang="pl">Dane geograficzne GPX</comment>
    <comment xml:lang="pt">Dados geográficos GPX</comment>
    <comment xml:lang="pt_BR">Dados geográficos GPX</comment>
    <comment xml:lang="ru">Географические данные GPX</comment>
    <comment xml:lang="sk">Zemepisné dáta GPX</comment>
    <comment xml:lang="sl">Geografski podatki GPX</comment>
    <comment xml:lang="sv">GPX-geografisk data</comment>
    <comment xml:lang="uk">географічні дані GPX</comment>
    <comment xml:lang="zh_CN">GPX 地理数据</comment>
    <acronym>GPX</acronym>
    <expanded-acronym>GPS eXchange Format</expanded-acronym>
    <generic-icon name="application-vnd-google-earth-kml"/>
    <glob pattern="*.gpx"/>
    <alias type="application/gpx"/>
    <alias type="application/gpx+xml"/>
    <alias type="application/x-gpx"/>
    <root-XML namespaceURI='http://www.topografix.com/GPX/1/0' localName='gpx'/>
    <root-XML namespaceURI='http://www.topografix.com/GPX/1/1' localName='gpx'/>
  </mime-type>
  <mime-type type="application/x-osm+xml">
    <sub-class-of type="application/xml"/>
    <comment>OSM Data</comment>
    <comment xml:lang="ca">Dades OSM</comment>
    <comment xml:lang="ca@valencia">Dades OSM</comment>
    <comment xml:lang="cs">Data OSM</comment>
    <comment xml:lang="de">OSM-Daten</comment>
    <comment xml:lang="en_GB">OSM Data</comment>
    <comment xml:lang="es">Datos OSM</comment>
    <comment xml:lang="eu">OSM datuak</comment>
    <comment xml:lang="fi">OSM-data</comment>
    <comment xml:lang="fr">Données « OSM »</comment>
    <comment xml:lang="it">Dati OSM</comment>
    <comment xml:lang="ko">OSM 데이터</comment>
    <comment xml:lang="lt">OSM duomenys</comment>
    <comment xml:lang="nl">Gegevens OSM</comment>
    <comment xml:lang="nn">OSM-data</comment>
    <comment xml:lang="pa">OSM ਡਾਟਾ</comment>
    <comment xml:lang="pl">Dane OSM</comment>
    <comment xml:lang="pt">Dados do OSM</comment>
    <comment xml:lang="pt_BR">Dados OSM</comment>
    <comment xml:lang="ru">Данные OSM</comment>
    <comment xml:lang="sk">Dáta OSM</comment>
    <comment xml:lang="sl">Podatki OSM</comment>
    <comment xml:lang="sv">OSM-data</comment>
    <comment xml:lang="uk">дані OSM</comment>
    <comment xml:lang="zh_CN">OSM 数据</comment>
    <acronym>OSM</acronym>
    <expanded-acronym>OpenStreetMap</expanded-acronym>
    <generic-icon name="application-vnd-google-earth-kml"/>
    <glob pattern="*.osm" />
    <glob pattern="*.osc" />
    <!-- no official namespaceURI defined for OSM XML, so cannot use root-XML tag -->
    <magic priority="80">
      <match value="&lt;osm" type="string" offset="0:256"/>
    </magic>
  </mime-type>
  <mime-type type="application/x-esri-shape">
    <generic-icon name="application-vnd-google-earth-kml"/>
    <comment>ESRI Shapefile</comment>
    <comment xml:lang="ca">Fitxers de formes ESRI</comment>
    <comment xml:lang="ca@valencia">Fitxers de formes ESRI</comment>
    <comment xml:lang="cs">Soubor tvaru ESRI</comment>
    <comment xml:lang="de">ESRI-Shapefile</comment>
    <comment xml:lang="en_GB">ESRI Shapefile</comment>
    <comment xml:lang="es">Archivo de formas ESRI</comment>
    <comment xml:lang="eu">ESRI forma-fitxategia</comment>
    <comment xml:lang="fi">ESRI Shapefile</comment>
    <comment xml:lang="fr">Fichier de formes « ESRI »</comment>
    <comment xml:lang="it">Shapefile ESRI</comment>
    <comment xml:lang="ko">ESRI 모양 파일</comment>
    <comment xml:lang="nl">Vormbestand ESRI</comment>
    <comment xml:lang="nn">ESRI-shapefile</comment>
    <comment xml:lang="pa">ESRI ਸ਼ਕਲ-ਫਾਇਲ</comment>
    <comment xml:lang="pl">Plik kształtu ESRI</comment>
    <comment xml:lang="pt">Ficheiro de formas da ESRI</comment>
    <comment xml:lang="pt_BR">Shapefile ESRI</comment>
    <comment xml:lang="ru">Файл фигуры ESRI</comment>
    <comment xml:lang="sk">Súbor tvarov ESRI</comment>
    <comment xml:lang="sl">ESRI Shapefile</comment>
    <comment xml:lang="sv">ESRI-formfil</comment>
    <comment xml:lang="uk">файл форм ESRI</comment>
    <comment xml:lang="zh_CN">ESRI 形状文件</comment>
    <generic-icon name="application-vnd-google-earth-kml"/>
    <glob pattern="*.shp"/>
    <magic priority="50">
      <match offset="0" type="big32" value="0x0000270a"/>
    </magic>
  </mime-type>
  <mime-type type="application/x-esri-shape-index">
    <comment>ESRI Shapefile Index</comment>
    <comment xml:lang="ca">Índex del fitxer de formes ESRI</comment>
    <comment xml:lang="ca@valencia">Índex del fitxer de formes ESRI</comment>
    <comment xml:lang="cs">Index souboru tvaru ESRI</comment>
    <comment xml:lang="de">ESRI-Shapefile-Index</comment>
    <comment xml:lang="en_GB">ESRI Shapefile Index</comment>
