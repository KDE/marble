{
    "id": "org.kde.marble",
    "branch": "master",
    "runtime": "org.kde.Platform",
    "runtime-version": "5.15-23.08",
    "sdk": "org.kde.Sdk",
    "command": "marble",
    "rename-icon": "marble",
    "tags": ["nightly"],
    "desktop-file-name-suffix": " (Nightly)",
